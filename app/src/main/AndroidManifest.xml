<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn.madingapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/smpyasmi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/smpyasmi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".DetailActivity" >
        </activity>
        <activity
            android:name=".SambutanActivity"
            android:label="@string/title_activity_sambutan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SejarahActivity"
            android:label="@string/title_activity_sejarah"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FasilitasActivity" />
        <activity android:name=".VisiMisiActivity" />
        <activity
            android:name=".ProfilActivity"
            android:label="@string/title_activity_profil"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UpdateActivity" />
        <activity android:name=".ViewActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>